proto messaging


message AESEncrypt{
    uint8[64] input = 1
    uint16 size = 2
    bool encrypt = 3
}

message AESResponse{
    uint8[64] response = 1
    uint16 size = 2
}

message PublicKeyExchange{
    uint8[32] pub_key = 1
}

enum MessageResponse : uint3 {
    ACK = 0
    NACK = 1
}

enum MesssageType : uint3 {
    PUBLIC_KEY_ECHANGE = 0
    
}

message Msg{
    MesssageType msg = 1
    MessageResponse res = 2
}